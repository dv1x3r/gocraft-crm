{
  "name": "w2go",
  "version": "1.0.0",
  "description": "This project has been created using **webpack-cli**, you can now run",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack --mode production",
    "build:tailwind": "tailwindcss -i web/index.css -o dist/index.css",
    "build:go": "templ generate && go build -o ./tmp/main ./main.go",
    "build": "concurrently npm:build:*",
    "watch:webpack": "webpack --mode development --watch",
    "watch:tailwind": "tailwindcss -i web/index.css -o dist/index.css --watch",
    "watch:go": "air",
    "watch": "concurrently npm:watch:*",
    "test:go": "go test -v ./...",
    "db:up": "dotenv -- goose -dir=./migrations up",
    "db:up-to": "dotenv -- goose -dir=./migrations up-to",
    "db:up-by-one": "dotenv -- goose -dir=./migrations up-by-one",
    "db:down": "dotenv -- goose -dir=./migrations down",
    "db:down-to": "dotenv -- goose -dir=./migrations down-to",
    "db:status": "dotenv -- goose -dir=./migrations status",
    "db:reset": "dotenv -- goose -dir=./migrations reset",
    "db:create": "dotenv -- goose -dir=./migrations create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "dotenv-cli": "^7.3.0",
    "htmx.org": "^1.9.7",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "w2ui": "^2.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}